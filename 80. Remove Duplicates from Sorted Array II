/**
* author: Darshika
* Question Link: https://leetcode.com/problems/remove-duplicates-from-sorted-array-ii/
* Time Complexity: O(n)
* Space Complexity: O(1)
*/

class Solution {
    public int removeDuplicates(int[] nums) {//[1,1,2,2,2,3]
        int i = 0;//2
        for (int n : nums)//1
            if (i < 2 || n > nums[i-2])
                nums[i++] = n;
        return i;
    }
}
