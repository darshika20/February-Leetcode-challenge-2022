/**
* author: Darshika
* Question Link: https://leetcode.com/problems/single-number/
* Time Complexity: O(n)
* Space Complexity: O(1)
*/

class Solution {
    public int singleNumber(int[] nums) {
        int xor = 0;
        for(int val : nums) {
            xor ^= val;
        }
        return xor;
    }
}
